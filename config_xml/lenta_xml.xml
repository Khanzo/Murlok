<?xml version="1.0" encoding="utf-8" ?>
<spider>
<name>lentaSpider</name>
<start_url>https://lenta.ru/rss</start_url>
<format>xml</format>
<encoding></encoding>
<agent></agent>
<selector>item</selector>
<maxpage>0</maxpage>
<nextpage></nextpage>
<nextselector></nextselector>
<endselector></endselector>
<properties>
<property>
<name>title</name>
<before></before>
<selector>title::text</selector>
<after></after>
<replace_filters>
<filter><old>  </old><new></new></filter>
</replace_filters>
</property>
<property>
<name>url_post</name>
<before></before>
<selector>link::text</selector>
<after></after>
<replace_filters></replace_filters>
</property>
<property>
<name>url_img</name>
<before></before>
<selector>enclosure::attr(url)</selector>
<after></after>
<replace_filters></replace_filters>
</property>
<property>
<name>date</name>
<before></before>
<selector>pubDate::text</selector>
<after></after>
<replace_filters></replace_filters>
</property>
<property>
<name>categ</name>
<before></before>
<selector>category::text</selector>
<after></after>
<replace_filters></replace_filters>
</property>
<property>
<name>site</name>
<before>lenta.ru</before>
<selector></selector>
<after></after>
<replace_filters></replace_filters>
</property>
<property>
<name>description</name>
<before></before>
<selector>description::text</selector>
<after></after>
<replace_filters></replace_filters>
</property>
</properties>
</spider>